{
  "name": "goetas-webservices/soap-client",
  "authors": [
    {
      "name": "Asmir Mustafic",
      "email": "goetas@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^7.1|^8.0",

    "psr/http-message": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-client": "^1.0",

    "psr/http-factory-implementation": "^1.0",
    "psr/http-message-implementation": "^1.0",
    "psr/http-client-implementation": "^1.0",

    "php-http/discovery": "^1.13",

    "symfony/dependency-injection": "^3.3|^4.0|^5.0",
    "doctrine/instantiator": "^1.0.3",
    "jms/serializer": "^1.6|^2.0|^3.0",

    "goetas-webservices/xsd2php-runtime": "^0.2.11",
    "goetas-webservices/soap-common": "^0.2.2",
    "goetas-webservices/soap-reader": "^0.3.3",
    "goetas-webservices/xsd2php": "^0.4.7",
    "laminas/laminas-code": "^4.0|^4.8",
    "laminas/laminas-zendframework-bridge": "^1.6|^1.7",
    "doctrine/inflector": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0|^8.0",
    "goetas-webservices/wsdl2php": "^0.5.3",
    "laminas/laminas-diactoros": "^2.4",
    "guzzlehttp/guzzle": "^7.2",
    "doctrine/coding-standard": "^8.1"
  },
  "autoload": {
    "psr-4": {
      "GoetasWebservices\\SoapServices\\SoapClient\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GoetasWebservices\\SoapServices\\SoapClient\\Tests\\": "tests",
      "GoetasWebservices\\Xsd\\XsdToPhp\\Tests\\": "vendor/goetas-webservices/xsd2php/tests",
      "GoetasWebservices\\WsdlToPhp\\Tests\\": "vendor/goetas-webservices/wsdl2php/tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3-dev"
    }
  },
  "bin": [
    "bin/soap-client"
  ],
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": false
    }
  }
}
