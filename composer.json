{
  "name": "goetas-webservices/soap-client",
  "authors": [
    {
      "name": "Asmir Mustafic",
      "email": "goetas@gmail.com"
    }
  ],
  "license": "MIT",
  "require": {
    "php": "^5.5|^7.0",

    "psr/http-message": "^1.0",
    "php-http/httplug": "^1.0",
    "php-http/discovery": "^1.0",
    "php-http/message-factory": "^1.0.2",
    "symfony/dependency-injection": "^2.4|^3.0",

    "doctrine/instantiator": "^1.0.3",

    "goetas-webservices/xsd2php-runtime": "^0.2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8|^5.0",

    "goetas-webservices/wsdl2php": "^0.4",
    "goetas-webservices/xsd2php": "^0.2.3",

    "jms/serializer": "dev-xml-namespaces-improvements as 1.5.0",

    "goetas-webservices/wsdl-reader": "^v0.3.1",

    "ass/xmlsecurity" : "^1.0",
    "php-http/guzzle6-adapter": "^1.0",
    "php-http/message": "^1.0"
  },
  "suggest": {
    "ass/xmlsecurity" : "Required for WS Security features"
  },
  "autoload": {
    "psr-4": {
      "GoetasWebservices\\SoapServices\\SoapClient\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GoetasWebservices\\SoapServices\\SoapClient\\Tests\\": "tests",
      "GoetasWebservices\\Xsd\\XsdToPhp\\Tests\\": "vendor/goetas-webservices/xsd2php/tests",
      "GoetasWebservices\\WsdlToPhp\\Tests\\": "vendor/goetas-webservices/wsdl2php/tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.2-dev"
    }
  },
  "bin": [
    "bin/soap-client"
  ]
}
